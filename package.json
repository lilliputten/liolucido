    "dev": "echo --Run nextjs server in development mode-- && next dev",
    "dev:https": "next dev --experimental-https",
    "build": "echo --Build the project-- && next build",
    "start": "echo --Start the project-- && next start",
    "lint": "next lint",
    "preview": "next build && next start",
    "prisma-check": "echo --Check prisma configuration-- && prisma validate",
    "prisma-format": "echo --Format prisma configuration file-- && prisma format",
    "prisma-generate": "echo --Generate database client-- && node prisma-switch.mjs && prisma generate",
    "prisma-db-push": "echo --Create database-- && prisma db push",
    "prisma-db-push-test": "echo --Create test database-- && cross-env DATABASE_URL='file:.data/test.db' prisma db push",
    "prisma-studio": "echo --Run database explorer-- && prisma studio",
    "husky-init": "husky init",
    "postinstall": "pnpm run prisma-generate && pnpm run prisma-db-push",
    "prepare": "husky",
    "test": "jest",
    "test-update-snapshots": "jest --updateSnapshot",
    "test-watch": "jest --watch",
    "test:ci": "jest --ci",
